função local SXVR_fake_script () -
Script TextLabel.LocalScript local = Instance.new ('LocalScript', TextLabel)

t local = 4; - quanto tempo leva para atravessar o arco-íris
enquanto wait () faz
matiz local = tick ()% t / t
cor local = Color3.fromHSV (matiz, 1, 1)
script.Parent.TextColor3 = color
fim

fim
coroutine.wrap (SXVR_fake_script) ()
função local ZHGMMIU_fake_script () -
script TextLabel.LocalScript local = Instance.new ('LocalScript', TextLabel)

script.Parent: TweenPosition (UDim2.new (0.5, -170,0,0), "In", "Quad", 0,5)
Aguarde (4)
script.Parent: TweenPosition (UDim2.new (0.5, -600,0,0), "In", "Quad", 0,5)
aguarde (2)
--script.Parent.Parent: Remove ( )
fim
coroutine.wrap (ZHGMMIU_fake_script) ()
fim
